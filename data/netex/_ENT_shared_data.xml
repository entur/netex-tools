                    </operatingDays>
                    <operatingPeriods>
                        <OperatingPeriod version="1" id="ENT:OperatingPeriod:1">
                            <FromDate>2025-03-11T00:00:00</FromDate>
                            <ToDate>2025-06-30T00:00:00</ToDate>
                        </OperatingPeriod>
                        <OperatingPeriod version="1" id="ENT:OperatingPeriod:2">
                            <FromDateRef ref="ENT:OperatingDay:2025-01-29"/>
                            <ToDateRef ref="ENT:OperatingDay:2025-04-02"/>
                        </OperatingPeriod>
                    </operatingPeriods>
                    <dayTypeAssignments>
                        <DayTypeAssignment order="1" version="1" id="ENT:DayTypeAssignment:1">
                            <Date>2025-05-02</Date>
                            <DayTypeRef ref="ENT:DayType:1" version="1"/>
                        </DayTypeAssignment>
                        <DayTypeAssignment order="2" version="1" id="ENT:DayTypeAssignment:2">
                            <OperatingPeriodRef ref="ENT:OperatingPeriod:1" version="1"/>
