                        </DayTypeAssignment>
                        <DayTypeAssignment order="2" version="1" id="ENT:DayTypeAssignment:3">
                            <OperatingDayRef ref="ENT:OperatingDay:2025-04-02" version="1"/>
                            <DayTypeRef ref="ENT:DayType:3" version="1"/>
                        </DayTypeAssignment>
                    </dayTypeAssignments>
                    <dayTypes>
                        <DayType version="1" id="ENT:DayType:1"/>
                        <DayType version="1" id="ENT:DayType:2">
                            <properties>
                                <PropertyOfDay>
                                    <DaysOfWeek>Monday Tuesday Wednesday Thursday</DaysOfWeek>
                                </PropertyOfDay>
                            </properties>
                        </DayType>
                        <DayType version="1" id="ENT:DayType:3"/>
                    </dayTypes>
                </ServiceCalendarFrame>
            </frames>
        </CompositeFrame>
    </dataObjects>
